   "source": [
    "class Solution:\n",
    "    def countAsterisks(self, s: str) -> int:\n",
    "       \n",
    "        l = s.split(\"|\")\n",
    "\n",
    "        w =l[0::2]\n",
    "\n",
    "        count = 0\n",
    "\n",
    "        for i in w:\n",
    "            for k in i:\n",
    "\n",
    "                if k == \"*\":\n",
    "                    count += 1\n",
    "\n",
    "        return count"
   ]
