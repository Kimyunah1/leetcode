   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def subsetXORSum(self, nums: List[int]) -> int:\n",
    "        res = 0\n",
    "        for i in nums:\n",
    "            res |= i\n",
    "\n",
    "        return res*(2**(len(nums)-1))"
   ]
  },
