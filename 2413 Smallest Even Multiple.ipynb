   "source": [
    "class Solution:\n",
    "    def smallestEvenMultiple(self, n: int) -> int:\n",
    "        \n",
    "        if n % 2 == 0:\n",
    "            n = n\n",
    "\n",
    "        else :\n",
    "            n = n*2\n",
    "\n",
    "        return n"
   ]
  },
