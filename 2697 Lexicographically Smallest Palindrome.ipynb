   "source": [
    "class Solution:\n",
    "    def makeSmallestPalindrome(self, s: str) -> str:\n",
    "        n = len(s)\n",
    "        l = \"\"\n",
    "\n",
    "        for a,b in zip(s[:n//2],s[::-1]):\n",
    "            l += min(a,b)\n",
    "\n",
    "        return l+s[n//2]*(n%2) + l[::-1]\n"
   ]
  },
