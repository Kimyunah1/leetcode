   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def maximumWealth(self, accounts: List[List[int]]) -> int:\n",
    "        l = []\n",
    "        for i in accounts:\n",
    "            l.append(sum(i))\n",
    "\n",
    "        return max(l)"
   ]
