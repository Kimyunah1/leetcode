   "source": [
    "class Solution:\n",
    "    def minOperations(self, s: str) -> int:\n",
    "        change=0\n",
    "        for i in range(0, len(s)):\n",
    "            if(int(s[i]) != i%2):\n",
    "                change += 1\n",
    "        return min(change, len(s)-change)"
   ]
  },
