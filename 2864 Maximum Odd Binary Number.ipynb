   "source": [
    "class Solution:\n",
    "    def maximumOddBinaryNumber(self, s: str) -> str:\n",
    "        countone = s.count('1')\n",
    "\n",
    "        return '1' * (countone - 1) + '0' * (len(s) - countone) + '1'\n"
   ]
  },
