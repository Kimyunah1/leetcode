   "source": [
    "class Solution:\n",
    "    def commonFactors(self, a: int, b: int) -> int:\n",
    "\n",
    "        list_a = [i for i in range(1, a+1) if a % i == 0]  \n",
    "        list_b = [i for i in range(1, b+1) if b % i == 0]  \n",
    "        \n",
    "        common = set(list_a) & set(list_b)  \n",
    "        \n",
    "        return len(common)"
   ]
  }
