   "source": [
    "class Solution:\n",
    "    def digitCount(self, num: str) -> bool:\n",
    "        \n",
    "        for i in range(len(num)):\n",
    "            if int(num[i]) != num.count(str(i)):\n",
    "               return False\n",
    "\n",
    "        return True\n",
    "                "
   ]
