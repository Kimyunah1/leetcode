   "source": [
    "class Solution:\n",
    "    def rotateString(self, s: str, goal: str) -> bool:\n",
    "        l = [ s[i+1:] + s[:i+1] for i in range(len(s)-1)] + [s]\n",
    "\n",
    "        return goal in l"
   ]
  },
