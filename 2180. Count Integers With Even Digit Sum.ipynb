   "source": [
    "class Solution:\n",
    "    def countEven(self, num: int) -> int:\n",
    "        ans = sum(1 for x in range(1, num + 1) if sum(int(digit) for digit in str(x)) % 2 == 0)\n",
    "        return ans\n",
    "\n"
   ]
  },
