   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def smallestRangeI(self, nums: List[int], k: int) -> int:\n",
    "        x = max(nums)\n",
    "        y = min(nums)\n",
    "\n",
    "        return max(0,(x-k)-(y+k))"
   ]
  },
