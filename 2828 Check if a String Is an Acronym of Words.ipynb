   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def isAcronym(self, words: List[str], s: str) -> bool:\n",
    "        l=\"\"\n",
    "        for word in words:\n",
    "            l += word[0]\n",
    "\n",
    "        return l == s"
   ]
  },
