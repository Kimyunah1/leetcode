   "source": [
    "from typing import List\n",
    "class Solution:\n",
    "    def maxProduct(self, nums: List[int]) -> int:\n",
    "        nn = sorted(nums,reverse=True)\n",
    "\n",
    "        return (nn[0] -1)*(nn[1]-1)"
   ]
  },
