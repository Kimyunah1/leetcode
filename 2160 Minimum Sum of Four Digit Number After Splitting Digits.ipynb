    "class Solution:\n",
    "    def minimumSum(self, num: int) -> int:\n",
    "       \n",
    "        new = sorted(str(num))\n",
    "        new1 = new[0] + new[2]\n",
    "        new2 = new[1] + new[3]\n",
    "\n",
    "        return int(new1) + int(new2)\n"
   ]
